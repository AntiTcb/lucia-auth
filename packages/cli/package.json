{
	"name": "@lucia-auth/cli",
	"version": "0.1.0",
	"description": "CLI for Lucia",
	"main": "index.js",
	"types": "index.d.ts",
	"module": "index.js",
	"type": "module",
	"files": [
		"**/*"
	],
	"scripts": {
		"build": "rm -rf ./dist/* && tsc && cp ./package.json ./dist && cp ./README.md ./dist && cp .npmignore dist",
		"package": "pnpm build && cd dist && pnpm install --no-frozen-lockfile && pnpm publish --no-git-checks --access public && cd ../",
		"ts": "ts-node --esm -T"
	},
	"keywords": [
		"lucia",
		"lucia-auth",
		"authentication",
		"auth",
		"CLI"
	],
	"bin": {
		"lucia": "scripts/index.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/pilcrowOnPaper/lucia-auth",
		"directory": "packages/lucia"
	},
	"author": "pilcrowonpaper",
	"license": "MIT",
	"exports": {
		"./package.json": "./package.json",
		".": "./index.js"
	},
	"devDependencies": {
		"@types/inquirer": "^9.0.3",
		"@types/node": "^18.11.9",
		"ts-node": "^10.9.1"
	},
	"peerDependencies": {
		"lucia-auth": "0.2.x"
	},
	"dependencies": {
		"inquirer": "^9.1.4"
	}
}
